YAAA,CAAA,IAAA;AACA,aAAA,UAAA,CAAA,QAAA,IAAA,oBAAA;AACA,aAAA,UAAA,GAAA,IAAA,CAAA,SAAA,CAAA,KAAA,UAAA,CAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,KAAA,UAAA;;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,UAAA,OAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,OAAA,CAAA,IAAA;AACA,SAFA;AAGA;AACA,KAbA;AAcA,IAAA,UAdA,wBAcA;AAAA;;AACA,MAAA,MAAA,CAAA,IAAA,uDACA,KAAA,MAAA,CAAA,MAAA,CAAA,IADA;;AAGA,MAAA,MAAA,CAAA,SAAA,GAAA,UAAA,OAAA,EAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,IAAA,CAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,OAAA,EAAA,KAAA,CAAA,YAAA,CAAA,KAAA;AACA,OAHA;AAIA;AAtBA,GAlBA;AA0CA,EAAA,OA1CA,qBA0CA;AACA,IAAA,WAAA,CAAA,KAAA,UAAA,CAAA;AACA;AA5CA,CAAA","sourcesContent":["<template>\n  <v-container fluid>\n    <v-card flat outlined class=\"mx-auto card-margin\">\n      <v-card-title>{{ form_details.description }}</v-card-title>\n      <v-card-subtitle class=\"card-subtitle-font-size\">{{ $route.params.name }}</v-card-subtitle>\n      <v-form ref=\"form\" class=\"form-padding\" lazy-validation>\n        <div v-for=\"parameter in form_details.parameters\" :key=\"parameter.name\">\n          <FormSelect\n            v-if=\"parameter.type == 'select'\"\n            :parameter=\"parameter\"\n            v-model=\"formSubmit[parameter.name]\"\n          />\n          <FormStringOrNumber\n            v-if=\"parameter.type == 'string' || parameter.type == 'number'\"\n            :parameter=\"parameter\"\n            v-model=\"formSubmit[parameter.name]\"\n          />\n          <FormCheckbox\n            v-if=\"parameter.type == 'bool'\"\n            :parameter=\"parameter\"\n            v-model=\"formSubmit[parameter.name]\"\n          />\n        </div>\n        <v-snackbar v-model=\"snackbar\" :timeout=\"timeout\">\n          {{ snackbarText }}\n          <template v-slot:action=\"{ attrs }\">\n            <v-btn color=\"blue\" text v-bind=\"attrs\" @click=\"snackbar = false\">Close</v-btn>\n          </template>\n        </v-snackbar>\n        <router-link to=\"/forms\">\n        <v-btn @click=\"validate\" style=\"float:right\" color=\"purple\">Submit</v-btn>\n        </router-link>\n      </v-form>\n    </v-card>\n  </v-container>\n</template>\n<script>\nimport { socket, socketCheck } from \"../plugins/websocket\";\nimport { EventBus } from \"../main\";\nimport FormSelect from \"./InputFields/FormSelect\";\nimport FormStringOrNumber from \"./InputFields/FormStringOrNumber\";\nimport FormCheckbox from \"./InputFields/FormCheckbox\";\n\nexport default {\n  data: () => {\n    return {\n      form_details: {},\n      checked: [],\n      strOrNum: [],\n      select: [],\n      formSubmit: {},\n      snackbar: false,\n      timeout: 2000,\n      snackbarText: \"There is an error in submitting the form.\"\n    };\n  },\n  components: {\n    FormSelect,\n    FormStringOrNumber,\n    FormCheckbox\n  },\n  methods: {\n    validate() {\n      this.snackbar = true;\n      if (this.$refs.form.validate()) {\n        this.snackbarText = \"Your form has been submitted.\";\n        this.formSubmit[\"form_name\"] = this.form_details.name;\n        this.formSubmit[\"action\"] = \"receive-form-specs\";\n        this.formSubmit = JSON.stringify(this.formSubmit);\n        socket.send(this.formSubmit);\n        socket.onmessage = message => {\n          console.log(message.data);\n        };\n      }\n    },\n    fetchSpecs() {\n      socket.send(\n        `{\"action\":\"send-form-specs\",\"name\": \"${this.$route.params.name}\"}`\n      );\n      socket.onmessage = message => {\n        this.form_details = JSON.parse(message.data);\n        EventBus.$emit(\"label\", this.form_details.label);\n      };\n    }\n  },\n  mounted() {\n    socketCheck(this.fetchSpecs);\n  }\n};\n</script>\n<style>\n.card-margin {\n  width: 500px;\n  margin-top: 10px;\n  padding-bottom: 34px;\n}\n.card-subtitle-font-size {\n  font-size: 16px !important;\n  font-weight: 500 !important;\n}\n.form-padding {\n  padding-left: 16px;\n  padding-right: 20px;\n  padding-top: 16px;\n  padding-bottom: 16px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}